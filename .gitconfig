[user]
  name = Kazuya Takeshima
  email = mail@mitukiii.jp
[color]
  ui = auto
  status = auto
  diff = auto
  branch = auto
  interactive = auto
  grep = auto
[color "diff"]
  plain = normal
  meta = bold green
  frag = bold cyan
  func = bold magenta
  old = bold red
  new = bold blue
  commit = bold white
  whitespace = normal magenta
[core]
  excludesfile = ~/.gitignore
  autocrlf = input
[alias]
  s    = status
  ss   = status -s
  sh   = show
  a    = add
  c    = commit
  ca   = commit -a
  cam  = commit --amend
  caam = commit -a --amend
  cp   = cherry-pick
  cpn  = cherry-pick -n
  co   = checkout
  cb   = checkout -b
  cm   = checkout master
  rb   = rebase
  rbm  = rebase master
  rbi  = rebase -i
  rbo  = rebase --onto
  rbc  = rebase --continue
  rba  = rebase --abort
  rs   = reset
  rss  = reset --soft
  rssh = reset --soft HEAD^
  rsh  = reset --hard
  rshh = reset --hard HEAD^
  m    = merge
  mn   = merge --no-ff
  br   = branch
  ba   = branch -a
  ft   = fetch
  pl   = pull
  ps   = push
  l    = log --stat --date=iso
  la   = log -p --date=iso
  ll   = log --graph --date=iso --all --pretty=format:'%h %Cgreen%ad %Cblue%cn %Creset%s %Cred%d'
  d    = diff
  dm   = diff master
  dw   = diff --color-words
  dc   = diff --cached
  gr   = grep -n
  gri  = grep -n -i
  grl  = grep -l
